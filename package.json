    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "connect-chaotic-response": "^1.0.3",
